/*******************************
         Theme Overrides
*******************************/

.ui.label {
  color: @darkGrey;
  background: transparent;
  
  &.basic {
    background: transparent;
    border-radius: 0;
    border: 0;
    color: @darkGrey;
  }
}

p.ui.label {
  padding: 0;
  width: 100%;
  margin-bottom: 10px;
}

span.ui.label {
  padding: 0;
}