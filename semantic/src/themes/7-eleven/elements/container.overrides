/*******************************
         Theme Overrides
*******************************/


.ui.container {
  padding: 10px;
  margin: 0;

  @media(min-width: 768px) {
    .header-label {
      padding: 35px 15px 14px;
    }

    .header-label span {
      font-size: 20px;
    }
  }

  @media (min-width: 1024px) {
    width: 1024px;
  }
}
